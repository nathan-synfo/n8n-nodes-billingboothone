{
    "name": "n8n-nodes-billingboothone",
    "version": "0.1.3",
    "description": "n8n community node for Billing Booth One API - comprehensive billing and customer management",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "billingbooth",
        "billing-booth-one",
        "billingboothone",
        "cdr-management",
        "customer-management"
    ],
    "license": "MIT",
    "homepage": "https://github.com/nathan-synfo/n8n-nodes-billingboothone",
    "repository": {
        "type": "git",
        "url": "https://github.com/nathan-synfo/n8n-nodes-billingboothone.git"
    },
    "bugs": {
        "url": "https://github.com/nathan-synfo/n8n-nodes-billingboothone/issues"
    },
    "author": {
        "name": "Nathan",
        "url": "https://github.com/nathan-synfo"
    },
    "scripts": {
        "build": "n8n-node build",
